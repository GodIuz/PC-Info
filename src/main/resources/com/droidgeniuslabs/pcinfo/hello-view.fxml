<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.droidgeniuslabs.pcinfo.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Button text="CPU Temp Monitor" onAction="#openCpuTemp" />
    <Button text="Internet Speed Test" onAction="#openSpeedTest"/>
    <Button text="System Info" onAction="#openSystemInfo"/>
    <Button text="Private / Public IP" onAction="#openGetIps"/>
</VBox>
